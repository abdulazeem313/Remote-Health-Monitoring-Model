.site-footer{
background-color:#222;
color:white;
margin-top:30px;
padding-top:20px;
}
.bottom-footer{
	border-top : 5px solid #b2b2b2;
	margin-top : 20px;
	padding-top : 10px;
	color : #b2b2b2;
	
}

.footer-nav 
{
	text-align : right;
	list-style : none;
}

.footer-nav li
{
	display: inline;
	
}

.footer-nav li:not(:first-child):before
{
	content : '|';
	padding : 0px 10px;
}

.footer-nav a
{
	color:#b2b2b2;
	
}

.footer-nav a:hover
{
	color:#fff;
	text-decoration :none;
}

.thumbnail img
{
	height:169px !important;
}
.content-container
{
	
	padding-top : 20px;
	
}
.no-margin
{
	margin:0px;
	padding:0px;
	border-radius:0px;
}

.main-image img
{
	height:360px;
	width:850px;
}

.thumbnail-left
{
	border-right :1px solid #b2b2b2;
}
.thumbnail-left a
{
	float:left;
	margin-right:5px;
}
.thumbnail-left img
{
	height:50px;
	width:50px;
}

.carousel-inner > .item > img,
  .carousel-inner > .item > a > img {
      width: 100%;
      height:380px;
      margin: auto;
  }
  
  